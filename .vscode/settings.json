{
    "cSpell.words": [
        "althack",
        "athackst",
        "codeql",
        "eamodio",
        "gemoji",
        "isort",
        "maint",
        "mkdocs",
        "mkdocsignore",
        "mkdocstrings",
        "pycache",
        "pyfakefs",
        "pygments",
        "pylint",
        "pymdownx",
        "pypi",
        "rogalmic",
        "shakram",
        "thackston",
        "unwatch",
        "venv",
        "VPNW",
        "yzhang"
    ],
    "cSpell.allowCompoundWords": true,
    "files.associations": {
        "*.bats": "shellscript"
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "editor.rulers": [
        80,
        127
    ],
    "yaml.customTags": [
        "tag:yaml.org,2002:python/name:materialx.emoji.twemoji",
        "tag:yaml.org,2002:python/name:materialx.emoji.to_svg",
        "tag:yaml.org,2002:python/name:pymdownx.emoji.gemoji",
        "tag:yaml.org,2002:python/name:pymdownx.emoji.to_png"
    ],
    "python.testing.unittestEnabled": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        "./tests",
        "-p",
        "test_*.py"
    ],
    "python.autoComplete.extraPaths": [
        "/home/vscode/.local/lib/python3.11/site-packages"
    ],
    "python.analysis.extraPaths": [
        "/home/vscode/.local/lib/python3.11/site-packages"
    ],
    "pylint.args": [
         "--init-hook='import sys; sys.path.append(\"/home/vscode/.local/lib/python3.11/site-packages\")'"
    ],
    "[python]": {
        "editor.tabSize": 4,
        "editor.wordBasedSuggestions": "off"
    },
    "pylint.importStrategy": "fromEnvironment",
}
