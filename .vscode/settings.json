{
  "grammarly.userWords": [
    "Buildx",
    "QEMU",
    "WORKDIR",
    "absmiddle",
    "action",
    "athackst",
    "autoremove",
    "bdist",
    "canonicalize",
    "changelog",
    "chmod",
    "cname",
    "devcontainer",
    "devcontainers",
    "elgohr",
    "emoji",
    "entrypoint",
    "fnmatch",
    "generator",
    "github",
    "heinrichreimer",
    "jfif",
    "markdownlint",
    "materialx",
    "md",
    "mkdir",
    "mkdocs",
    "mkdocstrings",
    "musl",
    "name",
    "nargs",
    "noninteractive",
    "noreply",
    "openssh",
    "peaceiris",
    "pgrep",
    "pkill",
    "pygments",
    "pylint",
    "pymdownx",
    "pymdownx emoji twemoji",
    "pypi",
    "python",
    "sdist",
    "setuptools",
    "squidfunk",
    "superfences",
    "thackston",
    "twemoji",
    "wheel"
  ],
  "files.associations": {
    "*.bats": "shellscript"
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "editor.rulers": [
    80,
    127
  ],
  "yaml.customTags": [
    "tag:yaml.org,2002:python/name:materialx.emoji.twemoji",
    "tag:yaml.org,2002:python/name:materialx.emoji.to_svg"
  ],
  "python.formatting.autopep8Args": [
    "--max-line-length=80",
    "--aggressive",
    "--aggressive",
    "--aggressive"
  ],
  "python.linting.pydocstyleArgs": [
    "--convention=google",
    "--add-ignore=D415"
  ],
  "python.linting.pylintArgs": [
    "--disable=W1514,W0613"
  ],
  "python.testing.unittestEnabled": true,
  "python.testing.unittestArgs": [
    "-v",
    "-s",
    "./tests",
    "-p",
    "*_test.py"
  ],
  "python.testing.pytestEnabled": false,
  "python.testing.nosetestsEnabled": false
}
