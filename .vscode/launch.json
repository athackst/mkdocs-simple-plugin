    "version": "0.2.0",
    "configurations": [
        {
            "name": "Build Mkdocs",
            "type": "python",
            "request": "launch",
            "program": "/usr/local/bin/mkdocs",
            "args": ["build", "--verbose"],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "name": "Build Mkdocs (mkdocs-test.yml)",
            "type": "python",
            "request": "launch",
            "program": "/usr/local/bin/mkdocs",
            "args": ["build", "--verbose"],
            "preLaunchTask": "debugger prelaunch",
            "cwd": "${fileDirname}",
            "console": "integratedTerminal"
        }
    ]
}
