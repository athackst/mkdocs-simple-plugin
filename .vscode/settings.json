        "-v",
        "-s",
        "./tests",
        "-p",
        "test_*.py"
    ],
    "python.autoComplete.extraPaths": [
        "/home/vscode/.local/lib/python3.11/site-packages"
    ],
    "python.analysis.extraPaths": [
        "/home/vscode/.local/lib/python3.11/site-packages"
    ],
    "[python]": {
        "editor.tabSize": 4,
        "editor.wordBasedSuggestions": "off"
    },
    "pylint.importStrategy": "fromEnvironment",
